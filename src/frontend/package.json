{
  "name": "holter-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "serve:prod": "vue-cli-service serve --port 3000 --mode prod",
    "build": "vue-cli-service build --mode prod",
    "build:prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "axios": "^0.21.4",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.18.1",
    "chart.js": "^2.9.4",
    "core-js": "^2.6.5",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-google-charts": "^0.3.3",
    "vue-router": "^3.4.8",
    "vue-toast-notification": "^0.6.2",
    "vuex": "^3.5.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^4.5.12",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
